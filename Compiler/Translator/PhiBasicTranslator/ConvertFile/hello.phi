phi.Hello:OS16BITVideo
{
	str hello: 'Hello, World!\r\n';
    str newline: '\r\n';
	str name:[40];
	
	log 'What is your name: ';
	ask name;
	log newline;
	log 'hello: ' name;
	log newline;
	log 'Press any key to continue...';
	
	call OS16BITVideo.WaitForKeyPress;
	
	call OS16BITVideo.EnableVideoMode;
	call OS16BITVideo.JumpToSectorTwo;
}